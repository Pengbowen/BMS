<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lanyuan.assembly.query.standard.DaoSandard" >
  <resultMap id="BaseResultMap" type="com.lanyuan.assembly.query.index.SingleIndex" >
    <id column="标准编号" property="standardNo" jdbcType="VARCHAR" />
    <result column="标准名称" property="standardName" jdbcType="VARCHAR" />
    <result column="标准名称（英）" property="standardNameEN" jdbcType="VARCHAR" />
    <result column="内容" property="content" jdbcType="VARCHAR" />
    <result column="标准分类" property="applicableMajor" jdbcType="VARCHAR" />
  </resultMap>
	<select id="selectAllStandard"  resultMap="BaseResultMap">
		select 标准编号,标准名称,标准名称（英）,内容,标准分类 from 标准总库 
	</select>

</mapper>