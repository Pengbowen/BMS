<!-- <?xml version="1.0" encoding="UTF-8"?> -->
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!--  -->
	<package name="standardrepor" namespace="/standardrepor" extends="GlobalInterceptor">
	   <!--添加 -->
		<action name="addStandardrepor" class="com.lanyuan.actionapi.standardrepor.IAddStandardReportAction" method="addStandardrepor">
			<result name="success" type="json"></result>
		</action>
		<!-- 修改 -->
		<action name="modifyStandardrepor" class="com.lanyuan.actionapi.standardrepor.IModifyLawsAction" method="modifyStandardrepor">
			<result name="success" type="json"></result>
		</action>
		<!-- 统计体系表数据  -->
		<action name="searchStatementData"  class="com.lanyuan.actionapi.standardrepor.ISearchStandardReportAction" method="searchStatistics" >
		  <result name="success" type="json"></result>
		</action>
		<!-- 统计标准总库和法规数据  -->
		<action name="searchStatement"  class="com.lanyuan.actionapi.standardrepor.ISearchStandardReportAction" method="statistics" >
		  <result name="success" type="json"></result>
		</action>
		    <!-- 显示前台统计页面  -->
    	<action name="frontStatementStandard" class="com.lanyuan.actionapi.standardrepor.IShowStandardReportAction">
    	   <result name="success">/statistics/frontStandardCount.jsp</result>
   	    </action>
	</package>
</struts>