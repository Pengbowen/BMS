<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="dictionarynew" namespace="/dictionarynew" extends="GlobalInterceptor">
		<!-- 衣物品牌brand_init、颜色color_init、瑕疵flaw_init、档次level_init、快慢服务speed_init,衣物附件attanch_init-->
		<action name="*_init" class="com.lanyuan.actionapi.commonmodule.dictionary.DictionaryNewShowAction">
			<interceptor-ref name="GlobalInterceptorStack" />
			<result name="success">/dictionarynew/dictionarynewList.jsp</result>
			<result name="error">/dictionarynew/dictionarynewList.jsp</result>
		</action>
        
        <!-- 新增、修改数据字典页面 -->
		<action name="showInfo" class="com.lanyuan.actionapi.commonmodule.dictionary.OperatorDictionaryNewAction" method="showInfo">
			<result name="success">/dictionarynew/dictionarynewAdd.jsp</result>
			<result name="error">/dictionarynew/dictionarynewAdd.jsp</result>
		</action>
		<!-- 新增数据字典信息 -->
		<action name="add" class="com.lanyuan.actionapi.commonmodule.dictionary.IAddDictionaryNewAction" method="add"/>
		<!-- 新增数据字典信息 -->
		<action name="checkCode" class="com.lanyuan.actionapi.commonmodule.dictionary.IAddDictionaryNewAction" method="checkCode"/>
		
		<!-- 修改数据字典信息 -->
		<action name="modify" class="com.lanyuan.actionapi.commonmodule.dictionary.IModifyDictionaryNewAction" method="modify">
		</action>
		
		<!-- 删除数据 -->     
		<action name="delete" class="com.lanyuan.actionapi.commonmodule.dictionary.IDeleteDictionaryNewAction" method="delete">
        </action>
        <!--显示查询数据  -->
        <action name="search" class="com.lanyuan.actionapi.commonmodule.dictionary.ISearchDictionaryNewAction" method="search">
   		</action>
		
		<!-- 终端访问接口 -->
		<action name="getData" class="com.lanyuan.actionapi.commonmodule.dictionary.ISelectDictionary" method="getData"></action>
		
	</package>
</struts>